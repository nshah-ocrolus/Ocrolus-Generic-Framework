{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "^/api/(.*)", "dest": "/api/index.js" },
    { "src": "^/launch$", "dest": "/public/launch.html" },
    { "src": "^/launch-app.js$", "dest": "/public/launch-app.js" },
    { "src": "^/launch-styles.css$", "dest": "/public/launch-styles.css" },
    { "src": "^/app.js$", "dest": "/public/app.js" },
    { "src": "^/styles.css$", "dest": "/public/styles.css" },
    { "src": "^/index.html$", "dest": "/public/index.html" },
    { "src": "^/$", "dest": "/public/index.html" }
  ]
}
