{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/launch", "dest": "/public/launch.html" },
    { "src": "/launch-app.js", "dest": "/public/launch-app.js" },
    { "src": "/launch-styles.css", "dest": "/public/launch-styles.css" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*\\.html)", "dest": "/public/$1" },
    { "src": "/(.*\\.css)", "dest": "/public/$1" },
    { "src": "/(.*\\.js)", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
